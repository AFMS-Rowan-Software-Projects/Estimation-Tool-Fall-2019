<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane maxHeight="330.0" maxWidth="810.0" minHeight="330.0" minWidth="810.0" prefHeight="330.0" prefWidth="810.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PM_Projects_Controller">
   <children>
      <Pane layoutX="-19.0" maxHeight="330.0" maxWidth="810.0" minHeight="330.0" minWidth="810.0" prefHeight="330.0" prefWidth="810.0">
         <children>
            <Pane layoutX="49.0" layoutY="65.0" prefHeight="240.0" prefWidth="752.0" style="-fx-border-color: black;">
               <children>
                  <TabPane fx:id="projectTabPane" layoutY="-31.0" prefHeight="272.0" prefWidth="754.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                      <Tab fx:id="unsubmittedTab" onSelectionChanged="#selectUnsubmitted" text="Unsubmitted">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <ScrollBar fx:id="unsubmittedScroll" layoutX="733.0" layoutY="4.0" orientation="VERTICAL" prefHeight="233.0" prefWidth="14.0" />
                                    <Pane fx:id="exampleProjectPane" layoutY="1.0" prefHeight="26.0" prefWidth="732.0" style="-fx-border-color: black;">
                                       <children>
                                          <Label fx:id="projectExampleLabel" layoutX="6.0" layoutY="5.0" text="Project Example 1 - Version 1.0.0">
                                             <font>
                                                <Font name="System Bold" size="12.0" />
                                             </font>
                                          </Label>
                                          <Text fx:id="editBtn" fill="#321ee5" layoutX="659.0" layoutY="18.0" onMouseClicked="#editProject" strokeType="OUTSIDE" strokeWidth="0.0" text="Edit" underline="true" />
                                          <Text fx:id="discardBtn" fill="#321ee5" layoutX="687.0" layoutY="18.0" onMouseClicked="#discardProject" strokeType="OUTSIDE" strokeWidth="0.0" text="Discard" underline="true" />
                                       </children>
                                    </Pane>
                                 </children>
                              </AnchorPane>
                        </content>
                      </Tab>
                      <Tab fx:id="submittedTab" onSelectionChanged="#selectSubmitted" text="Submitted">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <ScrollBar fx:id="submittedScroll" layoutX="733.0" layoutY="4.0" orientation="VERTICAL" prefHeight="233.0" prefWidth="14.0" />
                                 </children>
                              </AnchorPane>
                        </content>
                      </Tab>
                    </tabs>
                  </TabPane>
               </children>
            </Pane>
            <Button fx:id="newProjectBtn" layoutX="689.0" layoutY="37.0" mnemonicParsing="false" onAction="#addNewProject" text="Add New Project" />
            <Label layoutX="47.0" layoutY="9.0" text="Logged in as: " />
            <Label fx:id="usernameLabel" layoutX="123.0" layoutY="9.0" text="   Estimator">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>